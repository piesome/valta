{
  "name": "valta.data",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "git@github.com:piesome/valta.git",
  "author": "piesome",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json --type-check src/**/*.ts",
    "build": "npm-run-all build:tsc build:data",
    "build:tsc": "tsc",
    "build:data": "ts-node datapacker.ts",
    "watch": "npm-run-all build watch:data",
    "watch:data": "watch-run -p 'data/**' npm run build:data"
  },
  "dependencies": {
    "@types/node": "^7.0.12",
    "@types/ramda": "^0.0.6",
    "npm-run-all": "^4.0.2",
    "ramda": "^0.23.0",
    "ts-node": "^3.0.2",
    "tslint": "4",
    "typescript": "^2.2.2",
    "watch-run": "^1.2.5"
  }
}
